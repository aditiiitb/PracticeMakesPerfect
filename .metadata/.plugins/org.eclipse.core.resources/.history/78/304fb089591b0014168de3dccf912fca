
public class Program {
	public static void main(String args[])
	{
		System.out.println("helllooo");
		
		Node head = CreateList();
		
		PrintList(head);
	}
	
	private static Node CreateList()
	{
		Node head = new Node();
		head.Value = 10;
		Node next = new Node();
		next = head.Next;
		for(int i = 0; i < 10; i++)
		{
			next.Value = i;
			next.Next = new Node();
			next = next.Next;
		}
		return head;
	}
	
	private static void PrintList(Node head)
	{
		Node next = head.Next;
		while (next != null)
		{
			System.out.print(next.Value + " ");
		}
	}
}
